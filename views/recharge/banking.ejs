<!DOCTYPE html>

<html lang="en" class="light-style layout-menu-fixed" dir="ltr" data-theme="theme-default" data-assets-path="../assets/"
    data-template="vertical-menu-template-free">

<head>
    <meta charset="utf-8" />
    <meta name="viewport"
        content="width=device-width, initial-scale=1.0, user-scalable=no, minimum-scale=1.0, maximum-scale=1.0" />

    <title>Nạp tiền tài khoản</title>

    <meta name="description" content="" />

    <!-- Fonts -->
    <link rel="preconnect" href="https://fonts.googleapis.com" />
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin />
    <link
        href="https://fonts.googleapis.com/css2?family=Public+Sans:ital,wght@0,300;0,400;0,500;0,600;0,700;1,300;1,400;1,500;1,600;1,700&display=swap"
        rel="stylesheet" />

    <!-- Icons. Uncomment required icon fonts -->
    <link rel="stylesheet" href="/assets/vendor/fonts/boxicons.css" />

    <!-- Core CSS -->
    <link rel="stylesheet" href="/assets/vendor/css/core.css" class="template-customizer-core-css" />
    <link rel="stylesheet" href="/assets/vendor/css/theme-default.css" class="template-customizer-theme-css" />
    <link rel="stylesheet" href="/assets/css/demo.css" />

    <!-- Vendors CSS -->
    <link rel="stylesheet" href="/assets/vendor/libs/perfect-scrollbar/perfect-scrollbar.css" />

    <link rel="stylesheet" href="/assets/vendor/libs/apex-charts/apex-charts.css" />

    <!-- Page CSS -->

    <!-- Helpers -->
    <script src="/assets/vendor/js/helpers.js"></script>

    
    <script src="/assets/js/config.js"></script>
    <%- include('../main') %>
</head>

<body>
    <!-- Layout wrapper -->
    <div class="layout-wrapper layout-content-navbar">
        <div class="layout-container">
            <!-- Menu -->
            <%- include('../partials/sidebar', {index: 'banking' }) %>
                <!-- / Menu -->

                <!-- Layout container -->
                <div class="layout-page">
                    <!-- Navbar -->
                    <%- include('../partials/top') %>
                        <!-- / Navbar -->

                        <!-- Content wrapper -->
                        <div class="content-wrapper">
                            <!-- Content -->
                            <div class="container-xxl flex-grow-1 container-p-y">
                                <div class="row">
                                    <div class="col-12 card__chat">
                                        <div class="card shadow mb-4">
                                            <div class="card-header">
                                                <div class="btn-shine badge border-bottom border-2 border-blue p-1">
                                                    Nạp tiền
                                                </div>
                                            </div>
                                            <div class="card-body">
                                                <div class="nav-align-top">
                                                    <ul class="nav nav-pills mb-3 nav-fill" role="tablist">
                                                        <li class="nav-item mx-1">
                                                            <button type="button" class="nav-link border-1 active" role="tab"
                                                                data-bs-toggle="tab"
                                                                data-bs-target="#navs-pills-justified-home"
                                                                aria-controls="navs-pills-justified-home"
                                                                style="border: 1px #007bff solid !important;"
                                                                aria-selected="true">Nạp qua MoMo
                                                            </button>
                                                        </li>
                                                        <li class="nav-item mx-1">
                                                            <button type="button" class="nav-link border-1" role="tab"
                                                                data-bs-toggle="tab"
                                                                data-bs-target="#navs-pills-justified-profile"
                                                                aria-controls="navs-pills-justified-profile"
                                                                style="border: 1px #007bff solid !important;"
                                                                aria-selected="false">Nạp qua Bank
                                                            </button>
                                                        </li>
                                                    </ul>
                                                    <div class="tab-content">
                                                        <div class="tab-pane fade show active"
                                                            id="navs-pills-justified-home" role="tabpanel">
                                                            <div class="row">
                                                                <div class="col-12 col-md-5 mb-3">
                                                                    <h4 class="text-dark">Nạp qua MoMo <span
                                                                            class="fw-bolder text-warning">(Auto)</span>
                                                                        <img src="/assets/images/momo_logo.png"
                                                                            height="30" alt="momo_logo"></h4>
                                                                            <% momo.forEach(function(momos){ %>
                                                                        <div class="">
                                                                            <p class="text-dark">Số điện thoại: <span class="text-purple fw-bold cs-pointer" onclick="copyNoidung(); sweetCopy();"><%= momos.stk %> <i class="fa-solid fa-copy text-secondary"></i></span><input class="d-none" id="noidung" value="<%= momos.stk %>"></p>
                                                                        </div>
                                                                        <div class="">
                                                                            <p class="text-dark">Chủ tài khoản: <span class="text-purple fw-bold"><%= momos.ctk %></span></p>
                                                                        </div>
                                                                        <div class="">
                                                                            <p class="text-dark">Nội dung: <span class="text-purple fw-bold cs-pointer" onclick="copyCuphap(); sweetCopy();"><%= user.content %> <i class="fa-solid fa-copy text-secondary"></i></span><input class="d-none" id="cuphap" value="<%= user.content %>"></p>
                                                                        </div>
                                                                        <div class="">
                                                                            <p class="text-dark">Nạp tối thiểu: <span class="text-purple fw-bold"><%= momos.naptoithieu %>đ</span></p>
                                                                        </div>
                                                                    <% }); %>
                                                                    <p
                                                                        class="text-center fst-italic fw-lighter text-danger">
                                                                        Chú ý: Vui lòng chuyển khoản đúng nội dung
                                                                        '<%= user.content %>' để hệ thống tự động nạp tiền sau vài
                                                                        phút.</p>
                                                                    <div class="img d-none d-sm-block">
                                                                        <p class="text-center text-dark">
                                                                            <span class="fw-bold">Hoặc</span>: Quét mã QR dưới đây và điền chính xác nội dung
                                                                        </p>
                                                                        <div class="text-center">
                                                                            <img src="/assets/images/qr_momo.png" height="225" alt="qr_momo">
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                                <div class="col-12 col-md-7">
                                                                    <div class="home-notification-bank">
                                                                        <div class="table-responsive text-nowrap">
                                                                            <table class="table table-bordered">
                                                                                <thead class="table-active">
                                                                                    <tr class="text-center">
                                                                                        <th>ID</th>
                                                                                        <th>Trạng thái</th>
                                                                                        <th>Thời gian</th>
                                                                                        <th>Mã giao dịch</th>
                                                                                        <th>Số tiền</th>
                                                                                    </tr>
                                                                                </thead>
                                                                                <tbody>
                                                                                    <% lsmomo.forEach(function(momo){ %>
                                                                                    <tr class="text-center">
                                                                                        <td><%= momo.id %></td>
                                                                                        <td class="text-success fw-semibold">Hoàn thành</td>
                                                                                        <td><%= momo.time %></td>
                                                                                        <td class="text-danger fw-semibold"><%= momo.bankcode %></td>
                                                                                        <td><span class="text-success fw-semibold">+ <%= momo.realtake %> VNĐ</span></td>
                                                                                    </tr>
                                                                                    <% }); %>
                                                                                    <% if(lsmomo.length == 0){ %>
                                                                                        <tr class="odd">
                                                                                            <td valign="top" colspan="100%">
                                                                                                <center>
                                                                                                    <img src="/assets/images/no-data.png" alt="" width="120"
                                                                                                        height="120">
                                                                                                    <p>Không có dữ liệu để hiển thị</p>
                                                                                                </center>
                                                                                            </td>
                                                                                        </tr>
                                                                                        <% }; %>
                                                                                </tbody>
                                                                            </table>
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                        <div class="tab-pane fade" id="navs-pills-justified-profile"
                                                            role="tabpanel">
                                                            <div class="row">
                                                                <div class="col-12 col-md-5 mb-2">
                                                                    <h4 class="text-dark">Nạp qua MB Bank<span
                                                                            class="fw-bolder text-warning">(Không Auto)</span>
                                                                        <img src="/assets/images/mb_bank.png"
                                                                            height="30" alt="mb_bank"></h4>
                                                                            <% mb.forEach(function(mbs){ %>
                                                                        <div class="">
                                                                            <p class="text-dark">Số tài khoản: <span class="text-purple fw-bold cs-pointer" onclick="copyStk(); sweetCopy();"><%= mbs.stk %> <i class="fa-solid fa-copy text-secondary"></i></span><input class="d-none" id="stk" value="<%= mbs.stk %>"></p>
                                                                        </div>
                                                                        <div class="">
                                                                            <p class="text-dark">Chủ tài khoản: <span class="text-purple fw-bold"><%= mbs.ctk %></span></p>
                                                                        </div>
                                                                        <div class="">
                                                                            <p class="text-dark">Nội dung: <span class="text-purple fw-bold cs-pointer" onclick="copyCuphap(); sweetCopy();"><%= user.content %> <i class="fa-solid fa-copy text-secondary"></i></span><input class="d-none" id="cuphap" value="<%= user.content %>"></p>
                                                                        </div>
                                                                        <div class="">
                                                                            <p class="text-dark">Nạp tối thiểu: <span class="text-purple fw-bold"><%= mbs.naptoithieu %>đ</span></p>
                                                                        </div>
                                                                    <% }); %>
                                                                    <p
                                                                        class="text-center fst-italic fw-lighter text-danger">
                                                                        Chú ý: Vui lòng chuyển khoản với đúng nội dung
                                                                        '<%= user.content %>' và liên hệ với quản trị viên để được cộng tiền.</p>
                                                                </div>
                                                                <div class="col-12 col-md-7">
                                                                    <div class="home-notification-bank">
                                                                        <div class="table-responsive text-nowrap">
                                                                            <table class="table table-bordered">
                                                                                <thead class="table-active">
                                                                                    <tr class="text-center">
                                                                                        <th>ID</th>
                                                                                        <th>Thời gian</th>
                                                                                        <th>Mã giao dịch</th>
                                                                                        <th>Số tiền</th>
                                                                                        <th>Trạng thái</th>
                                                                                    </tr>
                                                                                </thead>
                                                                                <tbody>
                                                                                    <% lsmb.forEach(function(mb){ %>
                                                                                    <tr class="text-center">
                                                                                        <td><%= mb.id %></td>
                                                                                        <td><%= mb.time %></td>
                                                                                        <td><%= mb.bankcode %></td>
                                                                                        <td><span class="text-danger fw-semibold"><%= mb.realtake %></span> <span class="fw-bold">VNĐ</span></td>
                                                                                        <td class="text-success fw-semibold">Hoàn thành</td>
                                                                                    </tr>
                                                                                    <% }); %>
                                                                                    <% if(lsmb.length == 0){ %>
                                                                                    <tr class="odd">
                                                                                        <td valign="top" colspan="100%">
                                                                                            <center>
                                                                                                <img src="/assets/images/no-data.png" alt="" width="120"
                                                                                                    height="120">
                                                                                                <p>Không có dữ liệu để hiển thị</p>
                                                                                            </center>
                                                                                        </td>
                                                                                    </tr>
                                                                                    <% }; %>
                                                                                </tbody>
                                                                            </table>
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <!-- / Content -->

                            <!-- Footer -->
                            <%- include('../partials/footer') %>
                                <!-- / Footer -->

                                <div class="content-backdrop fade"></div>
                        </div>
                        <!-- Content wrapper -->
                </div>
                <!-- / Layout page -->
        </div>

        <!-- Overlay -->
        <div class="layout-overlay layout-menu-toggle"></div>
    </div>
    <!-- / Layout wrapper -->
    <!-- Core JS -->
    <!-- build:js assets/vendor/js/core.js -->
    <!-- SweetAlert -->

    <script src="/assets/vendor/libs/jquery/jquery.js"></script>
    <script src="/assets/vendor/libs/popper/popper.js"></script>
    <script src="/assets/vendor/js/bootstrap.js"></script>
    <script src="/assets/vendor/libs/perfect-scrollbar/perfect-scrollbar.js"></script>

    <script src="/assets/vendor/js/menu.js"></script>
    <!-- endbuild -->

    <!-- Vendors JS -->
    <script src="/assets/vendor/libs/apex-charts/apexcharts.js"></script>

    <!-- Main JS -->
    <script src="/assets/js/main.js"></script>

    <!-- Page JS -->
    <script src="/assets/js/dashboards-analytics.js"></script>
    <!-- Place this tag in your head or just before your close body tag. -->
    <script async defer src="https://buttons.github.io/buttons.js"></script>
</body>

</html>